#include <stdio.h>

int main() {
    int result;
    result = 2 + 3 * 4 - 1;

    printf("Expression: 2 + 3 * 4 - 1\n");
    printf("Evaluation order:\n");
    printf("1. Multiply 3 and 4: 3 * 4 = %d\n", 3 * 4);
    printf("2. Add 2 and 12: 2 + 12 = %d\n", 2 + 12);
    printf("3. Subtract 1 from 14: 14 - 1 = %d\n", 14 - 1);
    printf("Final result: %d\n", result);

    result = (2 + 3) * 4 - 1;

    printf("\nExpression: (2 + 3) * 4 - 1\n");
    printf("Evaluation order:\n");
    printf("1. Evaluate expression inside parentheses: 2 + 3 = %d\n", 2 + 3);
    printf("2. Multiply 5 and 4: 5 * 4 = %d\n", 5 * 4);
    printf("3. Subtract 1 from 20: 20 - 1 = %d\n", 20 - 1);
    printf("Final result: %d\n", result);

    // Example expression: 2 ^ 3 + 4 * 5 - 1
    result = (int)pow(2, 3) + 4 * 5 - 1;

    printf("\nExpression: 2 ^ 3 + 4 * 5 - 1\n");
    printf("Evaluation order:\n");
    printf("1. Exponentiation: 2 ^ 3 = %d\n", (int)pow(2, 3));
    printf("2. Multiply 4 and 5: 4 * 5 = %d\n", 4 * 5);
    printf("3. Add 8 and 20: 8 + 20 = %d\n", 8 + 20);
    printf("4. Subtract 1 from 28: 28 - 1 = %d\n", 28 - 1);
    printf("Final result: %d\n", result);

    return 0;
}
